<% if is_good_edit == true %>
　<div id="star_<%= modelname %>_edit"></div>
<script>
  var selectname = '#star_<%= modelname %>_edit'
  var elem = document.querySelector(selectname);
  elem.innerHTML = ""
  var option = {
      starOn: "<%= asset_path('star-on.png') %>",
      starOff: "<%= asset_path('star-off.png') %>",
      scoreName: "<%= modelname %>[good]"
  };
  raty(elem, option);
</script>

<% else %>
　<div id="star_<%= modelname %>_show_<%= model.id %>"></div>
  <script>
    var elem = document.querySelector('#star_<%= modelname %>_show_<%= model.id %>');
    elem.innerHTML = ""
    var option = {
      starOn: "<%= asset_path('star-on.png') %>",
      starOff: "<%= asset_path('star-off.png') %>",
      readOnly: true,
      score: <%= model.good %>
    };
    raty(elem, option);
  </script>

<% end %>

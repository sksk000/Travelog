<div class="col p-0 bg-white" data-management-target="user">
  <table class="table">
    <tr>
      <th scope="col">ユーザ名</th>
      <th scope="col">作成日時</th>
      <th scope="col">メールアドレス</th>
      <th scope="col">アカウント削除ボタン</th>
    </tr>
    <tbody>
      <% userall.each do | user |%>
        <tr>
        <td><%= user.name %></td>
        <td><%= user.created_at %></td>
        <td><%= user.email %></td>
        <td><%= link_to "削除" , admin_user_path(user.id),method: :delete, class: "btn btn-danger", data: {confirm: "削除しますか？"}%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
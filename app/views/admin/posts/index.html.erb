<div class="container">
  <div class="row">
    <div class="col">
      <table class="table">
        <tr>
          <th scope="col">タイトル</th>
          <th scope="col">投稿者名</th>
        </tr>
        <tbody>
          <% @post.each do |post| %>
            <tr>
            <td><%= link_to post.title ,admin_post_path(post.id) %></td>
            <td><%= post.user.name %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
